{
  "pull-request-header": "Release created",
  "pull-request-footer": "Enjoy",
  "include-component-in-tag": true,
  "include-v-in-tag": true,
  "tag-separator": "@",
  "separate-pull-requests": true,
  "draft": true,
  "packages": {
    "crates/goodbye-rust": {
      "plugins": [
        "cargo-workspace"
      ],
      "package-name": "rust-goodbye",
      "path": "crates/goodbye-rust",
      "release-type": "rust",
      "changelog-path": "crates/goodbye-rust/CHANGES.rst"
    },
    "charts/goodbye-helm": {
      "package-name": "helm-goodbye",
      "path": "charts/goodbye-helm",
      "release-type": "helm",
      "changelog-path": "charts/goodbye-helm/CHANGES.txt"
    },
    "crates/hello-rust": {
      "package-name": "hello-rust",
      "plugins": [
        "cargo-workspace"
      ],
      "path": "crates/hello-rust",
      "release-type": "rust"
    },
    "charts/hello-helm": {
      "package-name": "hello-helm",
      "path": "charts/hello-helm",
      "release-type": "helm"
    },
    ".": {
      "changelog-path": "CHANGELOG.md",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
